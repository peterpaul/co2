package compiler.test;

int c;

int fun_no_param()
{
	return 4 + 5;
}

int fun_one_param(int a, ...)
    return a + 5 + va_arg(int);

int fun_two_param(int a, int b, ...)
{
	return a + b + va_arg(int);
}

int fun_no_compound(int a, int b, ...)
    return a + b + va_arg(int);

void proc(int a, int b, ...)
{
	c = a + b + va_arg(int);
}

class A {
	int foo;
	int bar(...) {
		return foo + va_arg(int);
	}
}

int main()
{
	fun_no_param();
	fun_one_param(4);
	fun_two_param(4,5);
	proc(4,5);
	fun_one_param(4,6);
	fun_two_param(4,5,6);
	proc(4,5,6);
	return 0;
}