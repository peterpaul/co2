class MyObject : Object {
	int var;
	MyObject(int var) {
		self.var = var;
	}
	String to_string() {
		return new String("%d", var);
	}
}

int main()
{
	Object o = new MyObject(13);
	String s = o.to_string();
	s.fprint(stdout);
	return 0;
