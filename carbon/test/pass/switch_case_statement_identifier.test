["stdio.h"] int printf(char * format, ...);
["include.h"] int _ENUM_1, _ENUM_2;

int main() {
	int i;
	for (i = 0; i < 10; i +=1) {
		switch (i) {
		case _ENUM_1:
		case _ENUM_2:
			printf ("Number: %d!\n", i);
			break;
		default:
			printf ("Number: %d\n", i);
			break;
		}
	}
	return 0;
}
