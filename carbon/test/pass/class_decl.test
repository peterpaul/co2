class MyObject
{
	int var;
	int fun(int a, int b)
	{
		int c = a + b + var;
		return c;
	}
}

int main(int argc, char[][] argv)
{
	MyObject test = new MyObject();
	int val = test.var;
	int result = test.fun(val,5);
	return 0;
}