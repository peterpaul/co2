

int c;

int fun_no_param()
{
	return 4 + 5;
}

int fun_one_param(int a)
    return a + 5;

int fun_two_param(int a, int b)
{
	return a + b;
}

int fun_no_compound(int a, int b)
    return a + b;

void proc(int a, int b)
{
	c = a + b;
}

int fun_va_param(int a, ...)
{
	return a + va_arg(int);
}

int main()
{
	fun_no_param();
	fun_one_param(4);
	fun_two_param(4,5);
	fun_no_compound(4,5);
	proc(4,5);
	fun_va_param(4,5);
	return 0;
}