include co2/String
include co2/RefObject
include co2/Comparable

class Enum : RefObject, Comparable {
	String name;
	int value;

	Enum.private (int v, char * n) {
		super ();
		name = new String ("%s", n);
		name.retain ();
		value = v;
	}

	~Enum () {
		if (name) {
			name.release ();
			name = null;
		}
	}

	int compare (BaseObject other) {
		Enum otherEnum = (Enum) other;
		return value - otherEnum.value;
	}
}
